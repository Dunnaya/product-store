{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "README.md",
      "backend/",
      "frontend/src/"
    ],
    "includePattern": "\\.(js|jsx|md)$",
    "excludePattern": "(node_modules/|docs/|dist/|build/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "useLongnameInNav": true
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "readme": "README.md",
    "template": "./node_modules/docdash"
  }
}